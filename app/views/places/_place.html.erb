<div id="<%= dom_id place %>">
  <p>
    <strong>Latitude:</strong>
    <%= place.latitude %>
  </p>
  <p>
    <strong>Longitude:</strong>
    <%= place.longitude %>
  </p>
  <p>
    <strong>Zipcode:</strong>
    <%= place.zipcode %>
  </p>
  <p>
    <strong>Country:</strong>
    <%= place.country %>
  </p>
  <p>
    <strong>Weather Data:</strong>
    <%= place.weather_data %>
  </p>
</div>
