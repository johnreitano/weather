<%= form_with(model: place, id: "weather_form", local: true, data: { controller: "places", "places-target": "weather_form", action: "google-maps-loaded@window->places#initGoogle"}) do |form| %>
  <% if place.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(place.errors.count, "error") %> prohibited this place from being saved:</h2>
      <ul>
        <% place.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <div>
    <%= form.label :full_address, style: "display: block" %>
    <%= form.text_field :full_address, data: { "places-target": "full_address" } %>
  </div>
  <div>
    <%= form.label :latitude, style: "display: block" %>
    <%= form.text_field :latitude, data: { "places-target": "latitude" }
 %>
  </div>
  <div>
    <%= form.label :longitude, style: "display: block" %>
    <%= form.text_field :longitude, data: { "places-target": "longitude" }
 %>
  </div>
  <div>
    <%= form.label :zipcode, style: "display: block" %>
    <%= form.text_field :zipcode, data: { "places-target": "zipcode" }
 %>
  </div>
  <div>
    <%= form.label :country, style: "display: block" %>
    <%= form.text_field :country, data: { "places-target": "country" }
 %>
  </div>
<% end %>
